<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
    </PropertyGroup>
    
    <PropertyGroup Condition="'$(TUnitVersion)' == ''">
        <TUnitVersion>0.1.812</TUnitVersion>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="TUnit.Core" VersionOverride="$(TUnitVersion)" />
    </ItemGroup>

</Project>
